<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <!--<link rel="manifest" href="javascripts/pwa.webmanifest" />-->
        <link rel="manifest" href="javascripts/manifest.json" />
        <link rel="stylesheet" href="stylesheets/dashboard_admin.css" />
        <link
            rel="stylesheet"
            href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@24,400,0,200"
        />
        <title>dashboard admin</title>
    </head>
    <body>
        <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
        <div class="header">
            <span class="material-symbols-outlined logout" onclick="logout()"
                >logout</span
            >
        </div>
        <input type="checkbox" class="openSidebarMenu" id="openSidebarMenu" />
        <label for="openSidebarMenu" class="sidebarIconToggle">
            <div class="spinner diagonal part-1"></div>
            <div class="spinner horizontal"></div>
            <div class="spinner diagonal part-2"></div>
        </label>
        <div id="sidebarMenu">
            <ul class="sidebarMenuInner">
                <li id="absence">
                    <p>Absences</p>
                </li>
                <li id="list_diff">
                    <p>Liste de diffusion</p>
                </li>
                <li id="propositions">
                    <p>Propositions</p>
                </li>
                <li id="remplacements">
                    <p>Remplacements</p>
                </li>
            </ul>
        </div>
        <div class="container">
            <div id="container_absence"></div>
            <div id="absence_logo">
                <img
                    src="/assets/icons/logo_vaugelas.png"
                    alt="logo_vaugelas"
                />
            </div>
            <div id="container_list_diff">
                <!--
        <div class="list">
          <select name="prof">
            <option value="">--Liste des professeurs--</option>
            <option value="">M. Alasseur</option>
            <option value="">M. Perrod</option>
            <option value="">M. Lebourgt</option>
            <option value="">Mme. Anton</option>
            <option value="">Mme. Guillet</option>
            <option value="">Mme. Bonnard</option>
          </select>
        </div>
        -->
            </div>
            <div id="container_propositions">
                <!--
        <div class="list">
          <select name="prof">
            <option value="">--Liste des professeurs--</option>
            <option value="">M. Alasseur</option>
            <option value="">Mme. Anton</option>
          </select>
        </div>
        -->
            </div>
            <div id="container_remplacements">
                <!--
        <div class="list">
          <select name="prof">
            <option value="">M. Alasseur</option>
          </select>
        </div>
        -->
            </div>

            <div class="popUP hide">
                <div class="popUpContainer">
                    <p class="popUpText">Absence</p>
                    <p class="popUPCross">X</p>
                </div>
                <div class="container">
                    <div class="propositionList">
                        <h2 class="propostitionTitle">
                            Proposition sur cette absence
                        </h2>
                        <div class="popUPList"></div>
                    </div>
                    <div class="remplacementList">
                        <h2 class="remplacementTitle">
                            Remplacement pour cette absence
                        </h2>
                        <div class="popUPList"></div>
                    </div>
                </div>
            </div>
        </div>
    </body>
    <script src=""></script>
